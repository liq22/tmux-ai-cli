{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Run Extension (Tmux AI)",
      "type": "extensionHost",
      "request": "launch",
      "runtimeExecutable": "${execPath}",
      "args": [
        "--extensionDevelopmentPath=${workspaceFolder}/packages/vscode-tmux-ai",
        "${workspaceFolder}"
      ],
      "outFiles": [
        "${workspaceFolder}/packages/vscode-tmux-ai/dist/**/*.js"
      ],
      "preLaunchTask": "tmux-ai: compile extension",
      "env": {
        "TMUX_AI_DEBUG": "1"
      }
    }
  ]
}
